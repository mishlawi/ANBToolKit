:Family a owl:Class ;
    rdfs:label "Family" .

:birthDate a owl:DatatypeProperty ;
    rdfs:label "birth date" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .

:deathDate a owl:DatatypeProperty ;
    rdfs:label "death date" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .

:family-ontology a owl:Ontology ;
    rdfs:label "Family Ontology" .

:hasFilePath a owl:DatatypeProperty ;
    rdfs:label "has file path" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .

:hasFolder a owl:DatatypeProperty ;
    rdfs:label "has path" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .

:hasSubfolder a owl:ObjectProperty ;
    rdfs:label "has subfolder" ;
    rdfs:domain :Subfolder ;
    rdfs:range :Subfolder .

:hasChild a owl:ObjectProperty ;
    rdfs:label "has child" ;
    rdfs:domain :Person ;
    rdfs:range :Person ;
    owl:inverseOf :hasParent .

:hasParent a owl:ObjectProperty ;
    rdfs:label "has parent" ;
    rdfs:domain :Person ;
    rdfs:range :Person ;
    owl:inverseOf :hasChild .

:hasSpouse a owl:ObjectProperty ;
    rdfs:label "has spouse" ;
    rdfs:domain :Person ;
    rdfs:range :Person ;
    owl:inverseOf :hasSpouse .

