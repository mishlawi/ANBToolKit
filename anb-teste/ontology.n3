@prefix family: <http://example.org/family#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

family:Family a owl:Class ;
    rdfs:label "Family" .

family:family-ontology a owl:Ontology ;
    rdfs:label "Family Ontology" .

family:hasFolder a owl:DatatypeProperty ;
    rdfs:label "has path" ;
    rdfs:domain family:Person ;
    rdfs:range xsd:string .

family:Joaquina a family:Person ;
    rdfs:label "Joaquina" ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Joaquina"^^xsd:string ;
    family:hasSpouse family:Manuel .

family:Luis a family:Person ;
    rdfs:label "Luis" ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Luis"^^xsd:string ;
    family:hasSpouse family:Olga .

family:Olga a family:Person ;
    rdfs:label "Olga" ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Olga"^^xsd:string ;
    family:hasSpouse family:Luis .

family:hasChild a owl:ObjectProperty ;
    rdfs:label "has child" ;
    rdfs:domain family:Person ;
    rdfs:range family:Person ;
    owl:inverseOf family:hasParent .

family:hasParent a owl:ObjectProperty ;
    rdfs:label "has parent" ;
    rdfs:domain family:Person ;
    rdfs:range family:Person ;
    owl:inverseOf family:hasChild .

family:hasSpouse a owl:ObjectProperty ;
    rdfs:label "has spouse" ;
    rdfs:domain family:Person ;
    rdfs:range family:Person ;
    owl:inverseOf family:hasSpouse .

family:Ana a family:Person ;
    rdfs:label "Ana" ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Ana"^^xsd:string ;
    family:hasParent family:Adelino,
        family:Maria .

family:Adelino a family:Person ;
    rdfs:label "Adelino" ;
    family:hasChild family:Ana,
        family:Manuel ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Adelino"^^xsd:string ;
    family:hasSpouse family:Maria .

family:Manuel a family:Person ;
    rdfs:label "Manuel" ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Manuel"^^xsd:string ;
    family:hasParent family:Adelino,
        family:Maria ;
    family:hasSpouse family:Joaquina .

family:Maria a family:Person ;
    rdfs:label "Maria" ;
    family:hasChild family:Ana,
        family:Manuel ;
    family:hasFolder "/mnt/c/Users/Duarte Vilar/OneDrive/Ambiente de Trabalho/Eu/tese/thesis/Thesis/anb-teste/anb-family/Maria"^^xsd:string ;
    family:hasSpouse family:Adelino .

family:Person a owl:Class ;
    rdfs:label "Person" .

